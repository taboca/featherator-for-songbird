
/* Color table

  standard light - 250,190,240
  standard up  -255,210,250
  white - 255,240,255 
  
*/

/* Elected */

.color-normal {
        stop-opacity:1;
        stop-color:rgb(0,145,210);
}
.color-normal-less {
        stop-color:rgb(0,125,150);
        stop-opacity:1;
}
.color-normal-less-less {
        stop-color:rgb(0,85,130);
        stop-opacity:1;
}
.color-normal-more {
        stop-color:rgb(0,150,195);
        stop-opacity:1;
}
.color-normal-more-more {
        stop-color:rgb(0,175,225);
        stop-opacity:1;
}


.shadow-shadow {
        stop-opacity:.4;
        stop-color:rgb(0,0,0);
}
.shadow-transparent {
        stop-color:rgb(255,255,255);
        stop-opacity:0;
}

.bg-shadow {
        stop-opacity:.4;
        stop-color:rgb(0,0,0);
}
.bg-transparent {
        stop-color:rgb(255,255,255);
        stop-opacity:0;
}

.media-bg-from-color {
        stop-opacity:1;
        stop-color:rgb(0,130,160);
}
.media-bg-to-color {
        stop-color:rgb(0,100,130);
        stop-opacity:1;
}

.media-from-color {
        stop-color:rgb(0,120,150);
        stop-opacity:1;
}
.media-to-color {
        stop-color:rgb(0,100,130);
        stop-opacity:1;
}

.button-from-color {
	stop-color:rgb(0,150,180);
        stop-opacity:1;
}
.button-to-color {
        stop-color:rgb(0,120,160);
