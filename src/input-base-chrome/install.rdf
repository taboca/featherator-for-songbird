<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:songbird="http://www.songbirdnest.com/2007/addon-metadata-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>theme@featherator.taboca.com</em:id>
    <em:type>2</em:type>
    <em:name>PinkFeathers</em:name>
    <em:version>0.0.0.4</em:version>
    <em:creator>Marcio Galli</em:creator>
    <em:description>Pink Feathers for Songbird</em:description>
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id>
        <em:minVersion>0.8.0pre</em:minVersion>
        <em:maxVersion>1.1.0pre</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Register an skin for the built in layouts. 
         See http://wiki.songbirdnest.com/Developer/Developer_Intro/Feathers -->
    <songbird:skin>
      <Description>
        <songbird:name>PinkBird</songbird:name>
        <songbird:internalName>pinkbird</songbird:internalName>
        <songbird:compatibleLayout>
          <Description>
            <songbird:layoutURL>chrome://gonzo/content/xul/mainplayer.xul</songbird:layoutURL>
            <songbird:showChrome>false</songbird:showChrome>
            <songbird:onTop>false</songbird:onTop>
          </Description>
        </songbird:compatibleLayout>
        <songbird:compatibleLayout>
          <Description>
            <songbird:layoutURL>chrome://gonzo/content/xul/miniplayer.xul</songbird:layoutURL>
            <songbird:showChrome>false</songbird:showChrome>
            <songbird:onTop>true</songbird:onTop>
          </Description>
        </songbird:compatibleLayout>
      </Description>
    </songbird:skin>

  </Description>
</RDF>
